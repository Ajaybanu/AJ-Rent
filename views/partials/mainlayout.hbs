<script>
    // When user clicks the button, make an AJAX request to add item to wishlist
    $('.add-to-wishlist').click(function() {
    const itemID = $(this).data('item-id');

    $.ajax({
        url: '/wishlist',
        method: 'POST',
        data: { itemID: itemID },
        success: (response) => {
            console.log(response);
           alert(response.message);
        },
        error: () => {
            console.error('Error adding item to wishlist');
            alert(response.message);
        }
    });
});
</script>


