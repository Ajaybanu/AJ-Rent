     <main class="main">
        	<div class="page-header text-center" style="background-image: url('assets/images/page-header-bg.jpg')">
        		<div class="container">
        			<h1 class="page-title">Shopping Cart<span>Shop</span></h1>
        		</div><!-- End .container -->
        	</div><!-- End .page-header -->
           
{{Cart}}
            <div class="page-content">
            	<div class="cart">
	                <div class="container">
	                	<div class="row">
	                		<div class="col-lg-9">
	                			<table class="table table-cart table-mobile">
									{{#each Cart}}
									<thead>
										<tr>
											<th>{{this.name}}</th>
											<th>{{this.price}}</th>
											<th>{{this.quantity}}</th>
											{{!-- <th>{{this.image}}</th> --}}

											<th>Total</th>
											<th></th>
										</tr>
									</thead>
									{{/each}}

									<tbody>
										{{#each Cart}}
									
										<tr>
											<td class="product-col">
																							<div class="product">
													<figure class="product-media">
														<a href="/productdetails/{{this.id}}">
															<img src="{{this.image}}" alt="Product image">
														</a>
													</figure>
												</div><!-- End .product -->
	
											</td>
											<td class="price-col">Rs.{{this.price}}</td>
											<td class="quantity-col">
                                                <div class="cart-product-quantity">
                                                    <input type="number" class="form-control" value="1" min="1" max="10" step="1" data-decimals="0" required>
                                                </div><!-- End .cart-product-quantity -->
                                            </td>
											<td class="total-col">Rs.199</td>{{this.id}}
											<form action="/deletecart/{{this.id}}" method="post" >
											<td class="remove-col"><button class="btn-remove"><i class="icon-close"></i></button></td>
										</form>
										</tr>


										{{/each}}
									
									</tbody>
								</table><!-- End .table table-wishlist -->

	                			<div class="cart-bottom">
			            			<div class="cart-discount">
			            				<form action="#">
			            					<div class="input-group">
				        						<input type="text" class="form-control" required placeholder="coupon code">
				        						<div class="input-group-append">
													<button class="btn btn-outline-primary-2" type="submit"><i class="icon-long-arrow-right"></i></button>
												</div><!-- .End .input-group-append -->
			        						</div><!-- End .input-group -->
			            				</form>
			            			</div><!-- End .cart-discount -->

			            			<a href="#" class="btn btn-outline-dark-2"><span>UPDATE CART</span><i class="icon-refresh"></i></a>
		            			</div><!-- End .cart-bottom -->
	                		</div><!-- End .col-lg-9 -->
	                		<aside class="col-lg-3">
							<div class="summary summary-cart">
								<h3 class="summary-title">Proceed To Order</h3><!-- End .summary-title -->

								<table class="table table-summary">
									<tbody>{{#each cartProducts}}
										<tr class="summary-subtotal">
											<td>Name:</td>
											<td>{{this.name}}</td>
										</tr><!-- End .summary-subtotal -->
										<tr>
											<td>Price:</td>
											<td>{{this.price}}</td>
										</tr>
										
										<tr>
											<td>quantity:</td>
											<td>{{this.quantity}}</td>
										</tr>
										<tr>
											<td>Total:</td>
											<td>{{this.total}}</td>
										</tr>
										{{/each}}
										<tr>
                                            <td>apply Coupon:</td>
											<td>
												<form action="/checkout" method="post">
													
														{{#if coupdata}}
															<select id="category"  name="couponcode">
																{{#each coupdata}}
																<option>{{this.couponCode}}</option>
																{{/each}}
															</select>	
															{{else}}
															<h6>coupons not available</h6>
															{{/if}}
											</td>
										</tr>
										{{!-- <tr>
											<td>
											<div class="product-details-action">
											<button class="btn-product btn-cart" type="submit"> Apply</button>
											</div>
											</td>
										</tr> --}}
									            	
														
													
												
										<tr class="summary-total">
											<input type="hidden" value="{{totalPrice}}" name="totalPrice">
											<td>Total:</td>
											<td>{{totalPrice}}</td>
										</tr><!-- End .summary-total -->
									</tbody>
								</table><!-- End .table table-summary -->

							<button type="submit">	<a class="btn btn-outline-primary-2 btn-order btn-block">PROCEED TO
									CHECKOUT</a></button>
									</form>
							</div><!-- End .summary -->

							<a href="/viewproducts" class="btn btn-outline-dark-2 btn-block mb-3"><span>CONTINUE
									SHOPPING</span><i class="icon-refresh"></i></a>
						</aside><!-- End .col-lg-3 -->

	                	</div><!-- End .row -->
	                </div><!-- End .container -->
                </div><!-- End .cart -->
           
		    </div><!-- End .page-content -->
			
        </main><!-- End .main -->